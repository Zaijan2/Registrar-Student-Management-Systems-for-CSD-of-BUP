INSERT INTO classschedule (CourseID, InstructorID, Room, Day, Timeslot)
SELECT 
    (SELECT CourseID FROM Course ORDER BY RAND() LIMIT 1),       
    (SELECT InstructorID FROM instructor ORDER BY RAND() LIMIT 1),         
    concat('Room', Floor(RAND()*100)),                    
    ELT(FLOOR(1 + (RAND() * 7)), 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'),                                       
    SEC_TO_TIME(FLOOR(RAND()* 41400)+28800)
FROM
  (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 ) AS a,
  (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 ) AS b;
