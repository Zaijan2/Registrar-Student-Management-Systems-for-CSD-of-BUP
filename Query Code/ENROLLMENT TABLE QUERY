INSERT INTO enrollment (StudentID, CourseID, Semester, Year, Status)
SELECT 
    (SELECT StudentID FROM student ORDER BY RAND() LIMIT 1),       
    (SELECT CourseID FROM course ORDER BY RAND() LIMIT 1),         
    ELT(FLOOR(1 + (RAND() * 2)), '1st', '2nd'),                    
    FLOOR(RAND() * 5) + 2020,                                      
    ELT(FLOOR(1 + (RAND() * 3)), 'Enrolled', 'Dropped', 'Completed') 
FROM
  (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10 ) AS a,
  (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 8 UNION SELECT 9 UNION SELECT 10) AS b;
