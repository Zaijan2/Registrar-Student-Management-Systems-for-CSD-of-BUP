INSERT INTO course (CourseName, Description, Credits, DepartmentID )
SELECT 
    CONCAT('Course', FLOOR(RAND() *100)), 
    CONCAT('Description', FLOOR(RAND() * 100)),
    27,
    1
FROM
  (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 7) AS a,

 (SELECT 1 UNION SELECT 2 UNION SELECT 3 UNION SELECT 4 UNION SELECT 5 UNION
   SELECT 6 UNION SELECT 7) AS b;
